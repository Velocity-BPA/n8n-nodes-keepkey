{
  "name": "n8n-nodes-keepkey",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for KeepKey hardware wallet integration providing 23 resources and 150+ operations for Bitcoin, Ethereum, Cosmos, THORChain, and multi-chain asset management with ShapeShift swap integration.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-keepkey.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-keepkey#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-keepkey/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "keepkey",
    "hardware-wallet",
    "bitcoin",
    "ethereum",
    "cryptocurrency",
    "blockchain",
    "thorchain",
    "cosmos",
    "shapeshift",
    "defi",
    "swap",
    "multi-chain",
    "web3",
    "crypto",
    "hdwallet"
  ],
  "main": "dist/nodes/KeepKey/KeepKey.node.js",
  "types": "dist/nodes/KeepKey/KeepKey.node.d.ts",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/KeepKeyApi.credentials.js",
      "dist/credentials/KeepKeyNetwork.credentials.js"
    ],
    "nodes": [
      "dist/nodes/KeepKey/KeepKey.node.js",
      "dist/nodes/KeepKey/KeepKeyTrigger.node.js"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.32.2",
    "@cosmjs/proto-signing": "^0.32.2",
    "@cosmjs/stargate": "^0.32.2",
    "bitcoinjs-lib": "^6.1.5",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0",
    "ecpair": "^2.1.0",
    "ethers": "^6.9.0",
    "tiny-secp256k1": "^2.2.3",
    "usb": "^2.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
